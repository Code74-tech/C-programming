#include <stdio.h>
int main() {
    int num, sum = 0, temp, digit;
    printf("Enter number: ");
    scanf("%d", &num);
    temp = num;
    while(temp != 0) {
        digit = temp % 10;
        sum += digit * digit * digit;
        temp /= 10;
    }
    if(sum == num) printf("Armstrong");
    else printf("Not Armstrong");
    return 0;
}
