#include <stdio.h>
int main() {
    int i, num, max, min;
    printf("Enter 100 numbers:\n");
    scanf("%d", &num);
    max = min = num;
    for(i = 1; i < 100; i++) {
        scanf("%d", &num);
        if(num > max) max = num;
        if(num < min) min = num;
    }
    printf("Largest = %d, Smallest = %d", max, min);
    return 0;
}
