#include <stdio.h>
void findMinMax(int arr[], int n, int *min, int *max) {
    *min = *max = arr[0];
    for(int i = 1; i < n; i++) {
        if(arr[i] < *min) *min = arr[i];
        if(arr[i] > *max) *max = arr[i];
    }
}
int main() {
    int a[] = {5, 2, 9, 1, 7}, min, max;
    findMinMax(a, 5, &min, &max);
    printf("Min = %d, Max = %d\n", min, max);
    return 0;
}
