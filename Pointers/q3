#include <stdio.h>
#define SIZE 3

int main() {
    int mat[SIZE][SIZE];
    int *p = (int*)mat;
    printf("Enter 3x3 matrix:\n");
    for(int i=0; i<SIZE*SIZE; i++)
        scanf("%d", p+i);

    int max = *p, min = *p;
    for(int i=1; i<SIZE*SIZE; i++) {
        if(*(p+i) > max) max = *(p+i);
        if(*(p+i) < min) min = *(p+i);
    }

    printf("Max = %d, Min = %d\n", max, min);
    return 0;
}
