#include <stdio.h>
#include <ctype.h>
int main() {
    char str[100];
    int a=0, e=0, i=0, o=0, u=0;
    gets(str);
    for(int j = 0; str[j]; j++) {
        char ch = tolower(str[j]);
        if(ch=='a') a++;
        else if(ch=='e') e++;
        else if(ch=='i') i++;
        else if(ch=='o') o++;
        else if(ch=='u') u++;
    }
    printf("A=%d E=%d I=%d O=%d U=%d\n", a, e, i, o, u);
    return 0;
}
