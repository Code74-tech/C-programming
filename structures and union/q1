#include <stdio.h>
#include <string.h>

struct Student {
    int roll;
    char name[50];
    char course[50];
    char major[50];
    char minor[50];
};

void printNames(struct Student s[], int n) {
    for(int i = 0; i < n; i++)
        printf("Name: %s\n", s[i].name);
}

void printByRoll(struct Student s[], int n, int roll) {
    for(int i = 0; i < n; i++) {
        if(s[i].roll == roll) {
            printf("Roll: %d\nName: %s\nCourse: %s\nMajor: %s\nMinor: %s\n",
                   s[i].roll, s[i].name, s[i].course, s[i].major, s[i].minor);
            return;
        }
    }
    printf("Roll number not found.\n");
}

int main() {
    struct Student s[10] = {
        {101, "Amit", "BSc", "Physics", "Maths"},
        {102, "Neha", "BSc", "Chemistry", "Biology"},
        // Add more students as needed
    };
    printNames(s, 2);
    printByRoll(s, 2, 102);
    return 0;
}
